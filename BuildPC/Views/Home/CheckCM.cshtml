
@{
    ViewBag.Title = "CheckCM";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/FunctionCheckCM.js"></script>
<h2>Kiểm tra CPU và Mainboard</h2>

<div class="notifications-wrapper">
    <ul class="nav">
        <li class="dropdown">
            <ul class="list-group list-group-flush">
                <li><a href="@Url.Action("Check","Home")">Check PC</a></li>
                <li><a href="@Url.Action("CheckRM","Home")">Ram vs Mainboard</a></li>
                <li><a href="@Url.Action("CheckCM","Home")">CPU vs Mainboard</a></li>
                <li><a href="@Url.Action("CheckGM","Home")">GPU vs Mainboard</a></li>
                <li><a href="@Url.Action("CheckCG","Home")">CPU vs GPU</a></li>
            </ul>
        </li>
    </ul>
</div>

<p>Mainboard:</p>
<input list="lstmain" id="main" onclick="Test(this)">
<datalist id="lstmain"></datalist>

<p>CPU:</p>
<input list="lstcpu" id="cpu" onclick="Test(this)">
<datalist id="lstcpu"></datalist>
<hr />

<button onclick="CheckCM()">Check</button>

